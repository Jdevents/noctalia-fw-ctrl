{
  "id": "fw-control",
  "name": "Fan Control",
  "version": "1.0.0",
  "minNoctaliaVersion": "3.6.0",
  "author": "jdevents",
  "license": "MIT",
  "repository": "https://github.com/jdevents/noctalia-fw-ctrl",
  "description": "Control Framework fan profiles using fw-fanctrl, with optional ectool sensor/RPM display.",
  "entryPoints": {
    "main": "Main.qml",
    "barWidget": "BarWidget.qml",
    "panel": "Panel.qml",
    "settings": "Settings.qml"
  },
  "dependencies": {
    "plugins": []
  },
  "metadata": {
    "defaultSettings": {
      "pollMs": 2000,
      "profilesCsv": "lazy",
      "useEctool": false,
      "tempSensor": "cpu@4c",
      "showRpmInBar": true,
      "historyPoints": 60
    }
  }
}
